<Window x:Class="KoiKingdomPRN_WPF.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoiKingdomPRN_WPF"
        mc:Ignorable="d"
        Title="HomeWindow" Height="450" Width="800">
    <Grid>
        <local:HeaderWindow RenderTransformOrigin="0.5,0.5" Width="761" Margin="20,11,20,323" >
            <local:HeaderWindow.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.114"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.798"/>
                </TransformGroup>
            </local:HeaderWindow.RenderTransform>
        </local:HeaderWindow>
    </Grid>
</Window>
